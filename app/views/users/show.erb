<%if Helpers.is_logged_in?(@session)%>
<h1><%=@user.username%></h1>
<%@user.tweets.all.each do |tweet|%>
  <p><%=tweet.content%></p>
    <a href="/tweets/<%=@tweet.id%>/delete">Delete Tweet</a>
    <a href="/tweets/<%=@tweet.id%>/edit">Edit Tweet</a>
  <br></br>
  <%end%>
  <a href='/logout'>Log Out</a>
<%else%>
  <a href='/login'>Log In</a>
<%end%>
